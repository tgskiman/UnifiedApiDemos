<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="icon" href="@Url.Content("~/content/favicon.ico")" />
  <title>Unified API Demo</title>
  @Styles.Render("~/Content/css")
  @Scripts.Render("~/bundles/modernizr")
</head>
<body>
  <div class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        @Html.ActionLink("Unified API Demo", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
      </div>
      <div class="navbar-collapse collapse">
        <ul class="nav navbar-nav">
          <li>@Html.ActionLink("Tenant", "Index", "Tenant")</li>
          <li>@Html.ActionLink("Users", "Index", "Users")</li>
          <li>@Html.ActionLink("Groups", "Index", "Groups")</li>
          <li>@Html.ActionLink("Current User", "Index", "CurrentUser")</li>
          <li>@Html.ActionLink("Mail", "Index", "Mail")</li>
          <li>@Html.ActionLink("Calendar", "Index", "Calendar")</li>
          <li>@Html.ActionLink("Files", "Index", "Files")</li>
        </ul>
        @Html.Partial("_LoginPartial")
      </div>
    </div>
  </div>
  <div class="container body-content">
    @RenderBody()

  </div>

  @Scripts.Render("~/bundles/jquery")
  @Scripts.Render("~/bundles/bootstrap")
  @RenderSection("scripts", required: false)
</body>
</html>
